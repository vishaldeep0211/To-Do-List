
<%- include('header'); -%>
  <div class="box" id="heading">
    <h1> Work </h1>
  </div>

<div class="box">
  <% for(let i=0; i<newWorkItem.length; i++) {%>
  <div class="item">
    <input type="checkbox">
    <p><%= newWorkItem[i] %></p>
  </div>
    <% } %>
    <form class="item" action="/" method="post">
      <input type="text" name="newWorkItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value="Work">+</button>
    </form>
</div>

<%- include('footer'); -%>
